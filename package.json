{
  "name": "chocolate-ui",
  "version": "0.9.8",
  "description": "Indulge in silky smoothness",
  "author": "shiming_xsm",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "component",
    "ui",
    "react",
    "front-end"
  ],
  "homepage": "https://chocolateui.github.io/chocolate-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChocolateUI/chocolate-ui"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "0.1.14",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.8",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "react-transition-group": "^4.3.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build-ts && npm run build:css && npm run build-css",
    "build:css": "cd scripts && gulp",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:nowatch": "cross-env CI=true npm run test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublishOnly": "npm run test:nowatch && npm run lint && npm run build",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addons": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.8",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/classnames": "^2.2.10",
    "@types/highlight.js": "^10.1.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.2.3",
    "@types/storybook__addon-info": "^5.2.3",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filesize": "^0.0.6",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemap": "^1.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "6.0.1",
    "postcss-loader": "^5.2.0",
    "react": "^17.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-markdown": "^5.0.3",
    "react-scripts": "4.0.1",
    "react-syntax-highlighter": "^13.5.1",
    "rimraf": "^3.0.2",
    "sass-loader": "10.1.1",
    "style-loader": "^1.2.1",
    "typescript": "~3.9.7",
    "url-loader": "^4.1.0",
    "yarn-deduplicate": "^2.1.1"
  }
}
